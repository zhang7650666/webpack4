{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "chrome": "67"
            },
            "useBuiltIns": "usage"
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", {
            "corejs": 2,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
        }],
        "@babel/plugin-syntax-dynamic-import"
    ]
}

